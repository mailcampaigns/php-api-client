{
    "name": "mailcampaigns/api-client",
    "description": "MailCampaigns API-v3 client",
    "homepage": "https://www.mailcampaigns.nl/",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "MailCampaigns\\ApiClient\\": "src"
        }
    },
    "require": {
        "PHP": "^8.2",
        "ext-json": "*",
        "http-interop/http-factory-discovery": "^1.6"
    },
    "suggest": {
        "symfony/http-client": "Required to use Symfony HTTP Client."
    },
    "authors": [
        {
            "name": "Bert van der Genugten",
            "email": "bert@mailcampaigns.nl"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
